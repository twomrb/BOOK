            },
            "img": {
              "width": "(@mediaSize * 1px)",
              "height": "auto"
            },
            ".imgcenter": {
              "display": "block",
              "margin-left": "auto",
              "margin-right": "auto"
            },
            "& when not (@reverseContent)": {
              "@media (min-width: 992px)": {
                ".mbr-figure": {
                  "padding-right": "4rem"
                }
              },
              "@media (max-width: 991px)": {
                ".mbr-figure": {
                  "padding-bottom": "1rem"
                }
              }
            },
            "& when (@reverseContent)": {
