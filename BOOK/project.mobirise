              "position": "absolute",
              "left": "-3px",
              "bottom": "2px",
              "width": "16px",
              "height": "16px",
              "border": "2px solid #b0b0b0",
              "border-right-color": "rgba(0, 0, 0, 0)",
              "border-radius": "50%",
